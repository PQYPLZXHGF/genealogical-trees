@prefix fhkb: <http://www.example.com/genealogy.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

fhkb:DomainEntity a owl:Class ;
    rdfs:label "Domain Entity" .

fhkb:Partnership a owl:Class ;
    rdfs:label "Partnership"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass fhkb:Person ;
            owl:onProperty fhkb:hasPartner ],
        fhkb:DomainEntity .

fhkb:Marriage a owl:Class ;
    rdfs:label "Marriage" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( fhkb:Partnership [ a owl:Restriction ;
                        owl:onClass fhkb:Person ;
                        owl:onProperty fhkb:hasFemalePartner ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onClass fhkb:Person ;
                        owl:onProperty fhkb:hasMalePartner ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

fhkb:Sex a owl:Class ;
    rdfs:label "Sex" ;
    rdfs:subClassOf fhkb:DomainEntity ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( fhkb:Female fhkb:Male ) ] .

fhkb:Man a owl:Class ;
    rdfs:label "Man" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:hasSex ;
                        owl:someValuesFrom fhkb:Male ] ) ] .

fhkb:Woman a owl:Class .

fhkb:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( fhkb:DomainEntity [ a owl:Restriction ;
                        owl:onProperty fhkb:hasFather ;
                        owl:someValuesFrom fhkb:Man ] [ a owl:Restriction ;
                        owl:onProperty fhkb:hasMother ;
                        owl:someValuesFrom fhkb:Woman ] [ a owl:Restriction ;
                        owl:onProperty fhkb:hasSex ;
                        owl:someValuesFrom fhkb:Sex ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( fhkb:DomainEntity [ a owl:Restriction ;
                        owl:onProperty fhkb:hasFather ;
                        owl:someValuesFrom fhkb:Man ] [ a owl:Restriction ;
                        owl:onProperty fhkb:hasMother ;
                        owl:someValuesFrom fhkb:Woman ] [ a owl:Restriction ;
                        owl:onProperty fhkb:hasSex ;
                        owl:someValuesFrom fhkb:Sex ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                        owl:onClass fhkb:Person ;
                        owl:onProperty fhkb:hasParent ] ) ] .

owl:Thing a owl:Class ;
    rdfs:label "owl : Thing" .

fhkb:Male a owl:Class ;
    rdfs:label "Male" ;
    rdfs:subClassOf fhkb:Sex .

fhkb:Female a owl:Class ;
    rdfs:label "Female" ;
    rdfs:subClassOf fhkb:Sex ;
    owl:disjointWith fhkb:Male .

fhkb:Ancestor a owl:Class ;
    rdfs:label "Ancestor" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:isAncestorOf ;
                        owl:someValuesFrom fhkb:Person ] ) ] .

fhkb:MaleAncestor a owl:Class ;
    rdfs:label "Male Ancestor" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Man [ a owl:Restriction ;
                        owl:onProperty fhkb:isAncestorOf ;
                        owl:someValuesFrom fhkb:Person ] ) ] .

