fhkb:FemaleDescendant a owl:Class ;
    rdfs:label "Female Descendant" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Woman [ a owl:Restriction ;
                        owl:onProperty fhkb:hasAncestor ;
                        owl:someValuesFrom fhkb:Person ] ) ] .

fhkb:Cousin a owl:Class ;
    rdfs:label "Cousin" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:hasAncestor ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                            owl:onProperty fhkb:isSiblingOf ;
                                            owl:someValuesFrom [ a owl:Class ;
                                                    owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                owl:onProperty fhkb:isParentOf ;
                                                                owl:someValuesFrom fhkb:Person ] ) ] ] ) ] ] ) ] .

fhkb:FirstCousin a owl:Class ;
    rdfs:label "First Cousin" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:hasParent ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                            owl:onProperty fhkb:isSiblingOf ;
                                            owl:someValuesFrom [ a owl:Class ;
                                                    owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                owl:onProperty fhkb:isParentOf ;
                                                                owl:someValuesFrom fhkb:Person ] ) ] ] ) ] ] ) ] .

fhkb:SecondCousin a owl:Class ;
    rdfs:label "Second Cousin" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:hasParent ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                            owl:onProperty fhkb:hasParent ;
                                            owl:someValuesFrom [ a owl:Class ;
                                                    owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                owl:onProperty fhkb:isSiblingOf ;
                                                                owl:someValuesFrom [ a owl:Class ;
                                                                        owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                                    owl:onProperty fhkb:isParentOf ;
                                                                                    owl:someValuesFrom [ a owl:Class ;
                                                                                            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                                                        owl:onProperty fhkb:isParentOf ;
                                                                                                        owl:someValuesFrom fhkb:Person ] ) ] ] ) ] ] ) ] ] ) ] ] ) ] .

fhkb:ThirdCousin a owl:Class ;
    rdfs:label "Third Cousin" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                        owl:onProperty fhkb:hasParent ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:onProperty fhkb:hasParent ;
                                owl:someValuesFrom [ a owl:Class ;
                                        owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                    owl:onProperty fhkb:hasParent ;
                                                    owl:someValuesFrom [ a owl:Class ;
                                                            owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                        owl:onProperty fhkb:isSiblingOf ;
                                                                        owl:someValuesFrom [ a owl:Class ;
                                                                                owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                                            owl:onProperty fhkb:isParentOf ;
                                                                                            owl:someValuesFrom [ a owl:Class ;
                                                                                                    owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                                                                owl:onProperty fhkb:isParentOf ;
                                                                                                                owl:someValuesFrom [ a owl:Class ;
                                                                                                                        owl:intersectionOf ( fhkb:Person [ a owl:Restriction ;
                                                                                                                                    owl:onProperty fhkb:isParentOf ;
                                                                                                                                    owl:someValuesFrom fhkb:Person ] ) ] ] ) ] ] ) ] ] ) ] ] ) ] ] ] ) ] .
